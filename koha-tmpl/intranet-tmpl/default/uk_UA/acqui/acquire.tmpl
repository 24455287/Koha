				<td><!-- TMPL_VAR NAME="author" --></td>
				<td><!-- TMPL_VAR NAME="quantity" --></td>
				<td><!-- TMPL_VAR NAME="quantityrecieved" --></td>
			</tr>
		<!-- /TMPL_LOOP -->
		</table>
