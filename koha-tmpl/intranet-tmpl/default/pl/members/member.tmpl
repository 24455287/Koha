<div id="mainbloc">
<h1>Wyszukiwanie Czytelników</h1>

<div class="bloc100">
	<div class="bloctitle">Szukaj </div>
	<form action="/cgi-bin/koha/members/member.pl" method="post">
		<input type="text" name="member" value="<!-- TMPL_VAR NAME="member" -->">  order by 
		<select name="orderby">
			<option value="surname,firstname">Nazwisko</option>
			<option value="cardnumber">Cardnumber</option>
		</select>
		<input type="submit" class="button" value="OK">
	</form>
</div>

<div class="bloc100">
	<div class="bloctitle">Szukano <!-- TMPL_VAR NAME="member" --></div>
	<p class="resultcount"><!-- TMPL_VAR Name ="numresults" --> members were found.
	 </p>
	<table>
		<tr>
			<th>&nbsp;</th>
			<th>Karta</th>
			<th>Nazwa</th>
			<th>Oddział</th>
			<th>Kategoria</th>
			<th>Zaległe/Wypożyczone</th>
			<th>Opłaty</th>
			<th>Sort1</th>
			<th>Sort2</th>
		</tr>
		<!-- TMPL_LOOP NAME="resultsloop" -->
			<!-- TMPL_IF NAME="background" -->
				<tr bgcolor="#ffffcc">
			<!-- TMPL_ELSE -->
				<tr bgcolor="white">
			<!-- /TMPL_IF -->
				<td><!-- TMPL_VAR NAME="count" --></td>
				<td><!-- TMPL_VAR NAME="cardnumber" --></td>
				<td>
					<p><a href="/cgi-bin/koha/members/moremember.pl?bornum=<!-- TMPL_VAR NAME="borrowernumber" -->"><!-- TMPL_VAR NAME="surname" -->, <b><!-- TMPL_VAR NAME="firstname" --></b></a></p>
					<p><!-- TMPL_VAR NAME="streetaddress" --> <!-- TMPL_VAR NAME="city" --></p>
					<p><i><!-- TMPL_VAR NAME="borrowernotes" --></i></p>
