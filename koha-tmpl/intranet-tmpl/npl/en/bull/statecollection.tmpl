							<option value="<!-- TMPL_VAR name="value" -->" <!-- TMPL_IF name="selected" -->selected<!-- /TMPL_IF -->><!-- TMPL_VAR name="itemlocationname" --></option>
						<!-- /TMPL_LOOP -->
					</select>
				<!-- TMPL_ELSE -->
